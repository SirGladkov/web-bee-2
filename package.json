{
  "name": "web-bee-2",
  "version": "1.0.10",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "start": "vite preview",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "commit-msg": "npx --no-install commitlint --edit $1"
    }
  },
  "config": {
    "standard-version": {
      "types": [
        {
          "type": "feat",
          "section": "Features"
        },
        {
          "type": "fix",
          "section": "Bug Fixes"
        },
        {
          "type": "perf",
          "section": "Performance Improvements"
        },
        {
          "type": "refactor",
          "section": "Code Refactoring"
        },
        {
          "type": "docs",
          "section": "Documentation"
        },
        {
          "type": "style",
          "hidden": false
        },
        {
          "type": "chore",
          "hidden": false
        },
        {
          "type": "test",
          "hidden": false
        },
        {
          "type": "revert",
          "section": "Reverts"
        }
      ]
    },
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "dotenv": "^16.4.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.1.5",
    "standard-version": "^9.5.0",
    "typescript": "^5.7.3",
    "vite": "^6.1.0"
  }
}
